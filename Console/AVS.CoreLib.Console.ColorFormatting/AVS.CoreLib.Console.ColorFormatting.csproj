    <PackageTags>avs console ansicodes</PackageTags>
    <RepositoryUrl>https://github.com/avertiy/avs-core-lib-nuget/Console/</RepositoryUrl>
    <RepositoryType>public</RepositoryType>
	<AssemblyVersion>2.1.0.0</AssemblyVersion>
    <FileVersion>2.1.0.0</FileVersion>
