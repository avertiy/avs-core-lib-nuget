<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <AssemblyName>AVS.CoreLib.Console.ColorFormatting</AssemblyName>
    <RootNamespace>AVS.CoreLib.Console.ColorFormatting</RootNamespace>
    <Authors>Artem Vertii</Authors>
	<Copyright>Artem Vertii</Copyright>
    <Company>AV Stuido</Company>
    <Product>CoreLib</Product>
